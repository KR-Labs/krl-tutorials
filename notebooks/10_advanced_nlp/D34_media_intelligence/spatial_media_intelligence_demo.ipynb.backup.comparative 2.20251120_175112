      "\n",
      "âœ“ Sentiment analysis complete\n",
      "   Distribution:\n",
      "sentiment_deep\n",
      "neutral     150\n",
      "negative     61\n",
      "positive     10\n",
      "\n",
      "   Average aspect sentiments:\n",
      "      Workers: -0.136\n",
      "      Management: +0.011\n",
